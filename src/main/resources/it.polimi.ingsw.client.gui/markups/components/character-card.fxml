<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<fx:root type="javafx.scene.layout.Pane" xmlns:fx="http://javafx.com/fxml">
    <StackPane>
        <ImageView
                fitWidth="123"
                fitHeight="189"
                fx:id="imageView">
        </ImageView>
        <VBox>
            <HBox alignment="CENTER">
                <ImageView
                        fitWidth="35"
                        fitHeight="35"
                        fx:id="coin">
                    <Image url="/it.polimi.ingsw.client.gui/assets/coin.png"/>
                </ImageView>
            </HBox>
            <GridPane fx:id="students" style="-fx-padding: 40 40 40 40" hgap="5" vgap="5"/>
        </VBox>
    </StackPane>
</fx:root>
